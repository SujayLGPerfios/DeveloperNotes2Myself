<?xml version="1.0"?>
<!-- 
	Definition		: The appInfo element specifies information to be used by the application. This element must go within an annotation element.
	Syntax		: 
						<appInfo
							source=anyURL
						>
							Any well-formed XML content
						</appInfo>
	Attributes
			source	: Optional. A URI reference that specifies the source of the application information
	Notes: 
			1. Parent elements: annotation 
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:annotation>
  <xs:appInfo>W3Schools Note</xs:appInfo>
  <xs:documentation xml:lang="en">
  This Schema defines a W3Schools note!
  </xs:documentation>
</xs:annotation>
</xs:schema>
